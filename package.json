{
  "name": "@pipcook/datacook",
  "version": "0.1.0",
  "description": "A JavaScript library for feature engineering on datasets",
  "main": "dist/index.js",
  "dependencies": {
    "@tensorflow/tfjs-core": "^3.8.0",
    "@tensorflow/tfjs-layers": "^3.8.0",
    "jimp": "^0.16.1",
    "seedrandom": "^3.0.5"
  },
  "scripts": {
    "test": "npm run test:node",
    "test:node": "nyc mocha --timeout 60000 -r ts-node/register ./test/node/**/*.ts",
    "test:simple": "mocha --timeout 60000 -r ts-node/register",
    "test:browser": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "build": "npm run clean && npm run build:node && npm run build:wasm",
    "build:node": "tsc -b tsconfig.json",
    "build:browser": "npm run build:node && webpack --mode production",
    "build:wasm": "make",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "lint": "eslint ./src",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "patch": "npm version patch",
    "doc": "typedoc src/index.ts --name \"Datacook API\" --out ./doc --tsconfig ./tsconfig.ts --highlightTheme github-light"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imgcook/datacook"
  },
  "keywords": [
    "data-processing",
    "data-analysis",
    "data-manipulation",
    "analysis"
  ],
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/imgcook/datacook/issues"
  },
  "homepage": "https://github.com/imgcook/datacook#README",
  "devDependencies": {
    "@tensorflow/tfjs-backend-cpu": "^3.8.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "browserify": "16.5.2",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.1.0",
    "karma": "^6.3.4",
    "karma-browserify": "7.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.2.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.0.5",
    "webpack": "5.50.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
