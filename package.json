{
  "name": "@pipcook/datacook",
  "version": "0.0.1",
  "description": "A JavaScript library for feature engineering on datasets",
  "main": "dist/index.js",
  "dependencies": {
    "@tensorflow/tfjs-backend-cpu": "^2.7.0",
    "@tensorflow/tfjs-backend-wasm": "^2.7.0",
    "@tensorflow/tfjs-core": "^2.7.0",
    "@tensorflow/tfjs-layers": "^2.8.0",
    "jimp": "^0.16.1"
  },
  "scripts": {
    "test": "npm run test:node && npm run test:browser",
    "test:node": "nyc mocha --timeout 10000 -r ts-node/register ./test/node/**/*.ts",
    "test:browser": "npm run build:browser && karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "build": "npm run clean && npm run build:node && npm run build:browser && npm run build:wasm",
    "build:node": "tsc -b tsconfig.json",
    "build:browser": "npm run build:node && webpack --mode production",
    "build:wasm": "make",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "lint": "eslint ./src",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "patch": "npm version patch",
    "publish": "npm pack && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imgcook/datacook"
  },
  "keywords": [
    "data-processing",
    "data-analysis",
    "data-manipulation",
    "analysis"
  ],
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/imgcook/datacook/issues"
  },
  "homepage": "https://github.com/imgcook/datacook#README",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.1.0",
    "browser-env": "3.3.0",
    "browserify": "16.5.2",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.1.0",
    "karma": "5.2.3",
    "karma-browserify": "7.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^7.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-loader": "^5.1.2",
    "mocha-webpack": "^1.1.0",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "source-map-loader": "^1.0.0",
    "transform-loader": "^0.2.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
